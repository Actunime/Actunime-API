{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": false,
        "declaration": true,
        "moduleResolution": "node",
        "module": "CommonJS",
        "isolatedModules": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    }
}